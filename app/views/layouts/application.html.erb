<!DOCTYPE html>
<html>
  <head>
    <title>Friends</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= javascript_importmap_tags %>
    
  </head>

  <body>

    <%= render 'home/header' %>
    <div class="container">
    
      <% if notice or alert %>
        <br>
        <%= render 'home/alert' %>
      <% end %>



      <br>
      <%= yield %>
    </div>
  </body>
</html>
